        <wsdl:output message="tns:DisposePreparedQueryResponse" />
        <wsdl:fault name="fault" message="tns:SqlFault" />
    </wsdl:operation>
    <wsdl:operation name="SetJdbcConnectionProperty">
        <wsdl:documentation>Sets simple-valued property on JDBC connection with given handle.</wsdl:documentation>
        <wsdl:input message="tns:SetJdbcConnectionPropertyRequest" />
        <wsdl:output message="tns:SetJdbcConnectionPropertyResponse" />
        <wsdl:fault name="fault" message="tns:SqlFault" />
