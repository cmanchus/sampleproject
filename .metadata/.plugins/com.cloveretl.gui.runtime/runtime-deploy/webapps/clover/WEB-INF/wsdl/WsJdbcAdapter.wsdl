        <wsdl:fault name="fault" message="tns:SqlFault" />
    </wsdl:operation>
    <wsdl:operation name="DisposePreparedQuery">
        <wsdl:documentation>Closes PreparedStatement corresponding to given query.</wsdl:documentation>
