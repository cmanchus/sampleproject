    <wsdl:documentation>WS-JDBC adapter it to provide WS-remoted access to JDBC connections on
        the CloverDX Server. Only a subset of the JDBC features is available.</wsdl:documentation>
    <wsdl:operation name="CreateAdapter">
        <wsdl:documentation>Creates new JDBC adapter using given properties and answers its handle. The properties are
        supposed to be org.jetel.connection.jdbc.AbstractDBConnection's XML configuration properties and must represent
        valid connection configuration with respect to the CloverDX server environment.</wsdl:documentation>
        <wsdl:input message="tns:CreateAdapterRequest" />
        <wsdl:output message="tns:CreateAdapterResponse" />
        <wsdl:fault name="fault" message="tns:SqlFault" />
    </wsdl:operation>
    <wsdl:operation name="DisposeAdapter">
        <wsdl:documentation>Disposes JDBC adapter releasing all resources (Connections, ResultSets, PreparedStatements, ...) allocated
        during adapter usage.</wsdl:documentation>
        <wsdl:input message="tns:DisposeAdapterRequest" />
        <wsdl:output message="tns:DisposeAdapterResponse" />
        <wsdl:fault name="fault" message="tns:SqlFault" />
    </wsdl:operation>
    <wsdl:operation name="OpenJdbcConnection">
        <wsdl:documentation>Opens new JDBC connection to the database and answers its handle.</wsdl:documentation>
        <wsdl:input message="tns:OpenJdbcConnectionRequest" />
        <wsdl:output message="tns:OpenJdbcConnectionResponse" />
        <wsdl:fault name="fault" message="tns:SqlFault" />
    </wsdl:operation>
    <wsdl:operation name="CloseJdbcConnection">
        <wsdl:documentation>Closes JDBC connection identified by given handle.</wsdl:documentation>
        <wsdl:input message="tns:CloseJdbcConnectionRequest" />
        <wsdl:output message="tns:CloseJdbcConnectionResponse" />
        <wsdl:fault name="fault" message="tns:SqlFault" />
    </wsdl:operation>
    <wsdl:operation name="ExecuteQuery">
        <wsdl:documentation>Executes given SQL query and answers it result, which is either
        - JDBC ResultSet proxy for SELECT query
        - number of rows affected by update query
        - empty result for DDL query
        </wsdl:documentation>
        <wsdl:input message="tns:ExecuteQueryRequest" />
        <wsdl:output message="tns:ExecuteQueryResponse" />
        <wsdl:fault name="fault" message="tns:SqlFault" />
    </wsdl:operation>
    <wsdl:operation name="FetchNextResultData">
        <wsdl:documentation>Answers next data page for given query ResultSet.</wsdl:documentation>
        <wsdl:input message="tns:FetchNextResultDataRequest" />
