        <wsdl:output message="tns:GetAllColumnsResponse" />
        <wsdl:fault name="fault" message="tns:SqlFault" />
    </wsdl:operation>
    <wsdl:operation name="PrepareQuery">
        <wsdl:documentation>Prepares JDBC PreparedStatement and answers its handle.</wsdl:documentation>
        <wsdl:input message="tns:PrepareQueryRequest" />
        <wsdl:output message="tns:PrepareQueryResponse" />
        <wsdl:fault name="fault" message="tns:SqlFault" />
    </wsdl:operation>
    <wsdl:operation name="ExecutePreparedQuery">
        <wsdl:documentation>Executes prepared query and answers its result.</wsdl:documentation>
